<div class="contents-container">
  <div class="video-card">

    <div class="video-title">
      <h1>{{ foundVideo.title }}</h1>
    </div>

    <div class="video-player">
		<iframe class="video-player" src="{{foundVideo.videoUrl}}" frameborder="0" allowfullscreen></iframe>
	</div>

    <div class="video-description">
      <p>{{ foundVideo.description }}</p>
    </div>
  </div>
</div>